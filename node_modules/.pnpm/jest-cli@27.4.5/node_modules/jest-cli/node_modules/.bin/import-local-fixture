#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/navid/go/src/github.com/orchest/orchest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/navid/go/src/github.com/orchest/orchest/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../import-local@3.0.3/node_modules/import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../../../../import-local@3.0.3/node_modules/import-local/fixtures/cli.js" "$@"
fi
